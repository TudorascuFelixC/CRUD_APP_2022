
<!--Line command to include header-->
<%- include('include/_header') %> 
<!--/include header ends here-->

<!--Main site-->
<main id="site-main">
    <div class="container">
        <div class="box-nav d-flex justify-between">
            <a href="/add-user" class="border-shadow">
                <span class="text-gradient">Create New User <i class="fa-solid fa-user"></i></span>
            </a>
        </div>
        <!--Form handling-->
        <form action="/" method="POST">
            <table class="table">
                <thead class="thead-dark">
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>@Email</th>
                        <th>Gender</th>
                        <th>Status</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                   <%- include('include/_show') %>  
                </tbody>
            </table>
        </form> 
    </div>
</main>

<!--Line command to include foter-->
<%- include('include/_footer') %> 
<!--/Include footer ends here-->